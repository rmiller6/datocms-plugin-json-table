{
  "name": "datocms-plugin-json-table",
  "homepage": "https://github.com/voorhoede/datocms-plugin-json-table",
  "version": "2.0.4",
  "description": "A plugin for DatoCMS that makes it possible to add/insert a key and value and output a simple JSON table",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "prepack": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "datocms-plugin-sdk": "^0.7.4",
    "datocms-react-ui": "^0.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "build",
    "docs"
  ],
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "json-field",
    "key-value-pair",
    "json-table",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/datocms-plugin-json-table.git"
  },
  "author": "Voorhoede",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "JSON Table",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.jpg",
    "entryPoint": "build/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
